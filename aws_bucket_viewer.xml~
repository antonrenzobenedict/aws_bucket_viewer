<tool id="AwsBucketViewer" name="AWS Buckets Viewer">
	<description> This script will output available buckets given the respective access and secret key. </description>
	<command interpreter="python">
	         aws_bucket_viewer.py $access_key $secret_key $bucketList
 	</command>
 	<inputs>
                <param format="text" name="access_key" type="text" label="Access Key" />
                <param format="text" name="secret_key" type="text" label="Secret Key" />
        </inputs>
        <outputs>
                <data format="txt" name="bucketList" />
        </outputs>
	<help>
	       Input the exact access and secret key.
	</help>
	
</tool>
